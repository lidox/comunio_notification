<?xml version="1.0"?>
<settings
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
	xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<proxies>
		<proxy>
			<host>proxy-internet.localnet</host>
			<nonProxyHosts>127.0.0.1|localhost|kazan.priv.atos.fr|qlf-kazan.priv.atos.fr|portal.kazan.priv.atos.fr</nonProxyHosts>
		</proxy>
	</proxies>
	<profiles>
		<profile>
			<repositories>
				<repository>
					<id>kazan-repo</id>
					<name>Kazan repository</name>
					<url>http://kazan.priv.atos.fr/nexus/content/groups/public/</url>
				</repository>
			</repositories>
			<id>kazan</id>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>kazan</activeProfile>
	</activeProfiles>
<!--  	<pluginGroups>
		<pluginGroup>org.codehaus.cargo</pluginGroup>
	</pluginGroups>-->
</settings>
